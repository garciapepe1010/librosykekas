    <xsd:complexType name="com.ibm.ws.logmet.collector.internal.LogmetCollector"> 
        <xsd:annotation> 
            <xsd:documentation>Esta función recopila sucesos de fuentes especificadas y los reenvía al servidor de recogida de registros Bluemix utilizando el protocolo Lumberjack.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Recopilador de registros Bluemix</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="source" default="message" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica las fuentes utilizadas por el recopilador de registros Bluemix.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Origen</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="tag" type="xsd:string"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica las etiquetas asociadas a los sucesos desde diferentes orígenes.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Etiqueta</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="hostName" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nombre de host del servidor de recogida de registros Bluemix.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nombre de host</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="port" use="required"> 
            <xsd:annotation> 
                <xsd:documentation>Número de puerto del servidor de recogida de registros Bluemix.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Puerto</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="1"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="65535"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="tenant" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>ID de inquilino registrado en el servidor de recogida de registros de Bluemix.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>ID de inquilino</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="password" use="required" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>Contraseña del ID de inquilino que está registrado en el servidor de recogida de registros de Bluemix.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Contraseña</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxFieldLength" use="optional" default="2048"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica el número máximo de caracteres del campo de mensaje asociado a los sucesos para varios orígenes. Si especifica un valor de 0 o -1, el número máximo de caracteres es ilimitado.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Longitud máxima de campo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="-1"> 
                            </xsd:minInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
