                    <ext:label>Tamaño máximo del archivo de registro</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
