        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="eventName" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>El nombre exclusivo del suceso de auditoría. Por ejemplo: SECURITY_AUTHN o SECURITY_AUTHZ. </xsd:documentation> 
