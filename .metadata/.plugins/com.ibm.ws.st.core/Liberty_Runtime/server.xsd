    <xsd:complexType name="com.ibm.ws.http"> 
        <xsd:annotation> 
            <xsd:documentation>Propiedades de configuración para un punto final HTTP.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Punto final HTTP</ext:label> 
