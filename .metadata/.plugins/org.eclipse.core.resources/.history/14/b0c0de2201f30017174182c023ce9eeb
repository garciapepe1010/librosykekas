package com.adnaloy.librosykekas.basics.interfaces;


import java.util.List;

import javax.ejb.TransactionAttribute;
import javax.ejb.TransactionAttributeType;


import com.adnaloy.librosykekas.basics.Categoria;
import com.adnaloy.librosykekas.basics.EditorialFab;
import com.adnaloy.librosykekas.basics.Item;

public interface ItemLocal {
	
    public String getCode() ;

	public void setCode(String code);

	public String getTitulo();

	public void setTitulo(String titulo);

	public String getResena();

	public void setResena(String resena);

	public EditorialFab getEditorial();

	public void setEditorial(EditorialFabLocal editorial);

	public String getComentario();

	public void setComentario(String comentario);
	
	

	public Categoria getCategoria();


	public void setCategoria(CategoriaLocal categoria);



	public void reset();
	public void anado();
	public void actualizo();
	public void elimino();
	public Item buscaItem();
	public List <Item> findAllItems();

}
